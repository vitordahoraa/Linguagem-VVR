<FUNCTION_DECLARATION> ::= <TIPOS_FUNCTION> <ID> #13 DEL_ABREPARENTESES #5 <PARAMETRO_DECLARATION> DEL_FECHAPARENTESES #7 <ESCOPO> | <TIPOS_FUNCTION> <ID> #13 DEL_ABREPARENTESES #5 DEL_FECHAPARENTESES #7 <ESCOPO>;

<FUNCTION_CALLING> ::= <ID> #53 DEL_ABREPARENTESES <PARAMETRO_CALLING> DEL_FECHAPARENTESES  |  <ID> #53 DEL_ABREPARENTESES DEL_FECHAPARENTESES;

<PARAMETRO_CALLING> ::= <ID> DEL_VIRGULA <PARAMETRO_CALLING> | <ID> #15;

<RETURN> ::= KW_RETURN <EXP> #22 | KW_RETURN;
<TIPOS_FUNCTION> ::= KW_INT #1 | KW_DOUBLE #1 | KW_CHAR #1 | KW_STRING #1 | KW_BOOL #1 | KW_VOID #1;
<TIPOS_PARAMETRO> ::= KW_INT #3 | KW_DOUBLE #3 | KW_CHAR #3 | KW_STRING #3 | KW_BOOL #3;


<INPUT> ::= KW_INPUT DEL_ABREPARENTESES <ID> #22 DEL_FECHAPARENTESES;

<OUTPUT> ::= KW_OUTPUT DEL_ABREPARENTESES <EXP> #22 DEL_FECHAPARENTESES;
