<FOR> ::= KW_FOR DEL_ABREPARENTESES <FOR_EXP> DEL_PONTOEVIRGULA <EXP> #19 DEL_PONTOEVIRGULA <EXP>  DEL_FECHAPARENTESES #54 <ESCOPO>;
<FOR_EXP> ::=  <DECLARACAO> | <ATRIB>;
<DOWHILE> ::= KW_DO <ESCOPO> KW_WHILE DEL_ABREPARENTESES <EXP> #19 DEL_FECHAPARENTESES;
