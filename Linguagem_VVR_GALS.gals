<ESCOPO> ::= DEL_ABRECHAVES #4 <LISTA_COMANDO> DEL_FECHACHAVES #8 | DEL_ABRECHAVES #4 DEL_FECHACHAVES #8;


<COMANDO> ::= <DECLARACAO>  | <ATRIB> | <IF> | <FOR> | <DOWHILE> | <WHILE> | <RETURN> | <INPUT> | <OUTPUT> | <FUNCTION_CALLING> | <OP_INC> #30 <ID> #51| <ID> <OP_INC>#30 #51;

